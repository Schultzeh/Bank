<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="pane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.home.HomeController">
   <center>
      <VBox fx:id="accountsBox" alignment="CENTER" prefHeight="513.0" prefWidth="554.0" BorderPane.alignment="TOP_CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></VBox>
   </center>
   <top>
      <Pane prefHeight="87.0" prefWidth="629.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="homeButton" fitHeight="34.0" fitWidth="34.0" layoutX="6.0" layoutY="9.0" onMouseClicked="#goToHomeController" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@icons8-customer-80.png" />
               </image>
            </ImageView>
            <Label fx:id="nameLabel" layoutX="40.0" layoutY="14.0" prefWidth="158.0" text="Label">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="736.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../login/logo.png" />
               </image>
            </ImageView>
            <Label layoutX="559.0" layoutY="8.0" text="Sweden Bank">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </top>
   <left>
      <Accordion prefHeight="503.0" prefWidth="235.0" BorderPane.alignment="CENTER">
        <panes>
          <TitledPane animated="false" prefHeight="441.0" prefWidth="294.0" text="Kontoöversikt">
               <content>
                  <VBox fx:id="accountOverview" prefHeight="200.0" prefWidth="100.0" />
               </content>
          </TitledPane>
          <TitledPane fx:id="newTransaction" animated="false" text="Ny överföring">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="419.0" prefWidth="216.0">
                     <children>
                        <Label layoutX="23.0" layoutY="28.0" text="Från konto" />
                        <Label layoutX="21.0" layoutY="80.0" text="Till konto" />
                        <Label layoutX="29.0" layoutY="165.0" text="Belopp" />
                        <Label layoutX="29.0" layoutY="225.0" text="Meddelande" />
                        <TextField fx:id="transactionFromAccount" layoutX="23.0" layoutY="49.0" />
                        <TextField fx:id="transactionToAcc" layoutX="23.0" layoutY="101.0" />
                        <TextField fx:id="transactionAmount" layoutX="23.0" layoutY="189.0" />
                        <TextField fx:id="transactionMessage" layoutX="23.0" layoutY="246.0" />
                        <Button fx:id="transactionSend" layoutX="84.0" layoutY="294.0" mnemonicParsing="false" onAction="#makeTransaction" text="Skicka" />
                     </children></AnchorPane>
            </content>
          </TitledPane>
          <TitledPane animated="false" text="untitled 3">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </TitledPane>
        </panes>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" />
         </BorderPane.margin>
      </Accordion>
   </left>
</BorderPane>
